<application>
  <component name="XDebuggerSettings">
    <data-views>
      <option name="showLibraryStackFrames" value="false" />
      <sort-values>true</sort-values>
      <option name="valueLookupDelay" value="150" />
    </data-views>
    <general>
      <scroll-to-center>true</scroll-to-center>
    </general>
    <debuggers>
      <debugger id="dotnet_debugger">
        <configuration>
          <option name="breakOnUserUnhandledExceptions" value="true" />
          <option name="userUnhandledExceptionsIgnoreList">
            <list>
              <option value="System.OperationCanceledException" />
              <option value="System.Threading.Tasks.TaskCanceledException" />
              <option value="System.Threading.ThreadAbortException" />
              <option value="System.ArgumentNullException" />
              <option value="RabbitMQ.Client.Exceptions.OperationInterruptedException" />
              <option value="System.ApplicationException" />
            </list>
          </option>
        </configuration>
      </debugger>
      <debugger id="javascript">
        <configuration>
          <custom-object-presentation />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>